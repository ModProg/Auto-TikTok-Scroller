{
  "manifest_version": 2,
  "name": "Auto TikTok Scroller Extension",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./dist/content.js"]
    }
  ],
  "permissions": ["tabs"],
  "browser_action": {
    "default_popup": "./popup/popup.html",
    "default_icon": "./img/tiktok.png",
    "default_title": "Start scrolling."
  }
}
